<skin version="1.0.0">

	<layout>
		
		<image src="background.png" x="0" y="0"></image>
		<font id="TEXT" charW="6" charH="8" chars="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!@#$%^&amp;*()-_+=[]{}|\/:;,.&lt;&gt;?\' " src="font.png"></font>
		<display font="TEXT" id="SONGINFO" width="125" height="8" x="32" y="5" defaultText="E-PHONIC MP3-PLAYER - GET YOUR OWN FREE SKINNABLE MP3-PLAYER AT WWW.E-PHONIC.COM/MP3PLAYER"></display>

		<visualization width="73" height="12" x="224" y="2" color="0xFFFFD200" clickList="scope,wave,fft,peak">
			<scope blur="2"></scope>
			<wave blur="0"></wave>
			<fft blur="0"></fft>
			<peak src="peak.png" blur="0"></peak>
		</visualization>

		<button id="PREV" 	frameH="11" x="162" y="2" src="b_prev.png"></button>
		<button id="STOP" 	frameH="11" x="174" y="2" src="b_stop.png"></button>
		<button id="PLAY" 	frameH="11" x="186" y="2" src="b_play.png"></button>
		<button id="PAUSE" 	frameH="11" x="198" y="2" src="b_pause.png"></button>
		<button id="NEXT" 	frameH="11" x="210" y="2" src="b_next.png"></button>
		
		<ephonic_icon x="2" y="2"></ephonic_icon>

	</layout>

</skin>